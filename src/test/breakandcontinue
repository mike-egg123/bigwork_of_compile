fn main() -> void {
    let counter:int = 4;
    while counter > 0 {
        counter = counter - 1;
        if counter == 8 {
            counter = 4;
        }else if counter == 0 {
            break;
        }else if counter == 2{
            continue;
        }else if counter == 2{
            continue;
        }
    }
}